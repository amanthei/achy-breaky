{
  "name": "Achy Breaky",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Remove the ‚ù§ from embedded gists",
  "homepage_url": "https://github.com/amanthei/achy-breaky",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*", "https://*/*"
      ],
      "js": [
        "src/inject/inject.js"
      ],
      "all_frames": true
    }
  ]
}
